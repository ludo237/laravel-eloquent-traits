{
  "name": "ludo237/laravel-eloquent-traits",
  "description": "Useful set of Eloquent traits",
  "license": "MIT",
  "authors": [
    {
      "name": "Claudio Ludovico",
      "email": "ludovico1990@hotmail.it"
    }
  ],
  "homepage": "https://gitlab.com/ludo237/laravel-eloquent-traits",
  "keywords": [
    "laravel",
    "eloquent",
    "orm",
    "active-record",
    "laravel-package",
    "open-source"
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "5.6.x|5.7.x|5.8.x|6.x"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "laravel/framework": "5.6.x|5.7.x|5.8.x|6.x",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.6",
    "phpunit/phpunit": "^7.3|^8.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Ludo237\\EloquentTraits\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ludo237\\EloquentTraits\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [],
      "aliases": {}
    }
  }
}
